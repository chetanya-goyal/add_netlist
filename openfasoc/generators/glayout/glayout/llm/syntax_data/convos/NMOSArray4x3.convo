# this trains the LLM to array from bottom left to top right
# first row
// move array components
move element2 to the right of element1
move element3 to the right of element2
# second row
move element4 above element1
move element5 above element1
move element6 above element1
move element5 to the right of element4
move element6 to the right of element5
# third row
move element7 above element4
move element8 above element4
move element9 above element4
move element8 to the right of element7
move element9 to the right of element8
# fourth row
move element10 above element7
move element11 above element7
move element12 above element7
move element11 to the right of element10
move element12 to the right of element11
